{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "g++.exe build active file",
            "command": "C:\\msys64\\mingw64\\bin\\g++.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe",
                "-I", "${workspaceFolder}\\BACKEND\\ImageProcessor\\install\\include",
                "-L", "${workspaceFolder}\\BACKEND\\ImageProcessor\\install\\x64\\mingw\\lib",
                "-lopencv_core4120",
                "-lopencv_imgcodecs4120",
                "-lopencv_highgui4120",
                "-lopencv_imgproc4120",
                "-lopencv_face4120"
            ],
            "options": {
                "cwd": "C:\\msys64\\mingw64\\bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
